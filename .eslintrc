{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["airbnb", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "@typescript-eslint/indent": [2, 2]
  }
}